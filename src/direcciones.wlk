
object movimientos {
	method moverUp(objeto){
		objeto.position(objeto.position().down(200))
	}
	
	method moverDown(objeto,number){
		objeto.position(objeto.position().down(200))
	}
	
	method moverRight(objeto,number){
		objeto.position(objeto.position().right(200))
	}
	
	method moverLeft(objeto,number){
		objeto.position(objeto.position().left(20))
	}
	
}
